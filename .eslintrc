{
  "extends": "semistandard",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module"
  },
  "plugins": [
    "babel"
  ],
  "rules": {
    "generator-star-spacing": 0,
    "babel/generator-star-spacing": 1,
    "space-before-function-paren": [2, "never"],
    "object-curly-newline": ["error", {
      "minProperties": 1
    }],
    "sort-imports": ["error", {
      "ignoreCase": false
    }],
    "sort-vars": ["error", {
      "ignoreCase": true
    }]
  }
}
